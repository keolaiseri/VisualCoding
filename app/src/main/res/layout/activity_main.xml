<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/iphone_14_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FFFFFF">

    <TextView
        android:id="@+id/ready_to_st"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="88dp"
        android:layout_centerVertical="true"
        android:layout_marginEnd="88dp"
        android:fontFamily="@font/sfpronormal"
        android:gravity="top"
        android:text="@string/ready_to_st"
        android:textAppearance="@style/ready_to_st" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/continue_Button"
        android:layout_below="@+id/ready_to_st"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="230dp"
        android:background="@drawable/button"
        android:drawableStart="@drawable/forwardarrow"
        android:fontFamily="@font/sfpronormal"
        android:paddingStart="18dp"
        android:paddingEnd="18dp"
        android:text="@string/continue_label"
        android:textAppearance="@style/label" />

